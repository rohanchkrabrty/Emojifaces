{
    "manifest_version": 2,
    "name": "Emojifaces",
    "description": "This will put a emoji on all images that have a face, in a webpage",
    "version": "0.1.0",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["imoji.js"]
        }
      ],
      "background": {
        "scripts": ["background.js"]
      },
      "browser_action": {
        "default_icon": {
          "16": "icons/icon16.png",
          "24": "icons/icon24.png",
          "32": "icons/icon32.png"
        },
        "default_title": "Imoji"
      },
      "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
       "128": "icons/icon128.png"
      }
}