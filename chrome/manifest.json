{
    "manifest_version": 2,
    "name": "Imoji",
    "version": "0.1.0",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["imoji.js"]
        }
      ],
      "background": {
        "scripts": ["background.js"]
      },
      "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Imoji"
      }
}